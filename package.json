{
  "name": "@damon.huang/directory-analyzer",
  "version": "1.0.9",
  "main": "dist\\index.js",
  "scripts": {
    "prettier": "npx prettier --config ./.prettierrc --write .",
    "build": "npx tsc --build tsconfig.json",
    "serve": "npx ts-node \".\\src\\index.ts\"",
    "lint": "eslint \"src/**/*.+(ts|tsx)\"",
    "lintfix": "npm run lint -- --fix",
    "precommit": "npm run prettier && npm run lint",
    "test": "jest --detectOpenHandles --colors",
    "prepublish": "npm run precommit && npm run build && npm test && echo \"update version and commit code\""
  },
  "keywords": [],
  "author": "damon.huang",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": [
      "npm run prettier",
      "git add"
    ],
    "*.+(ts|tsx)": [
      "npm run lint",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.2",
    "@types/shelljs": "^0.8.7",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-recommended": "^4.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^25.5.3",
    "lint-staged": "^10.1.7",
    "prettier": "^2.0.5",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "fs-extra": "^9.0.0",
    "moment": "^2.24.0",
    "shelljs": "^0.8.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DamonMuscle/directory-analyzer.git"
  },
  "bugs": {
    "url": "https://github.com/DamonMuscle/directory-analyzer/issues"
  },
  "homepage": "https://github.com/DamonMuscle/directory-analyzer#readme",
  "description": ""
}
