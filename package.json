{
  "name": "@damon.huang/directory-analyzer",
  "version": "1.0.2",
  "main": "dist\\index.js",
  "scripts": {
    "prettier": "npx prettier --config ./.prettierrc --write .",
    "build": "npx tsc --build tsconfig.json",
    "serve": "npx ts-node \".\\src\\index.ts\"",
    "lint": "eslint \"src/**/*.+(ts|tsx)\"",
    "lintfix": "npm run lint -- --fix",
    "precommit": "npm run prettier && npm run lint",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "damon.huang",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": [
      "npm run prettier",
      "git add"
    ],
    "*.+(ts|tsx)": [
      "npm run lint",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/node": "^13.13.2",
    "@types/shelljs": "^0.8.7",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-recommended": "^4.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.7",
    "prettier": "^2.0.5",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "moment": "^2.24.0",
    "shelljs": "^0.8.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DamonMuscle/osMonitor.git"
  },
  "bugs": {
    "url": "https://github.com/DamonMuscle/osMonitor/issues"
  },
  "homepage": "https://github.com/DamonMuscle/osMonitor#readme",
  "description": ""
}
